package testemesa;

import java.util.Scanner;

public class TesteMesa {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = 4;
		x = 0;
		y = 4;
		
		while (x<3) {
			y += 2;
			x += 1;
			
			System.out.println(x + "-" + y);
		}


	}

}
